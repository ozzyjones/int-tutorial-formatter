{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "object-literal-key-quotes": [true, "always"],
        "quotemark": [true, "single", "avoid-escape"],
        "trailing-comma": [true, {"objects": "never"}],
        "variable-name": [true, "check-format", "allow-leading-underscore"]
    },
    "rulesDirectory": []
}