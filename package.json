{
    "name": "int-tutorials",
    "displayName": "int-tutorials",
    "description": "General formatting for INT tutorials",
    "version": "0.0.2",
    "publisher": "Brian",
    "engines": {
        "vscode": "^1.21.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.formatTutorials"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.formatTutorials",
                "title": "INT: Format Tutorials"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "mocha -r ts-node/register -u tdd src/**/*.test.ts"
    },
    "devDependencies": {
        "@types/chai": "^4.1.2",
        "@types/mocha": "^2.2.48",
        "@types/node": "^7.0.43",
        "chai": "^4.1.2",
        "eslint": "^4.19.1",
        "grunt": "^1.0.2",
        "grunt-tslint": "^5.0.1",
        "html-entities": "^1.2.1",
        "htmlparser2": "^3.9.2",
        "js-beautify": "^1.7.5",
        "mocha": "^5.0.5",
        "titlecase": "^1.1.2",
        "ts-node": "^5.0.1",
        "tslint": "^5.9.1",
        "typescript": "^2.6.1",
        "vscode": "^1.1.6"
    },
    "dependencies": {}
}
